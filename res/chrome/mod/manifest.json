{
	"manifest_version": 2,
	"name": "Void Mod",
	"version": "0.1.0",
	"offline_enabled": true,
	"content_scripts": [{
		"matches": ["<all_urls>"],
		"css": ["scrollbar.css"],
		"js": []
	}],
	"chrome_url_overrides" : {
        "newtab": "newtab.html"
    },
	"permissions": [
        "activeTab",
        "tabs",
        "http://*/*",
        "https://*/*",
		"notifications",
		"contextMenus",
		"bookmarks",
		"history"
    ]
}
