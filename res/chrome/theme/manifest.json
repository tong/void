{
    "manifest_version": 2,
    "name": "Void",
    "version": "0.1.0",
    "description": "Dark Void",
    "theme": {
        "images": {
            "theme_frame": "images/frame.png",
            "theme_toolbar": "images/toolbar.png",
            "theme_tab_background": "images/tab.png",
            "theme_ntp_background": "images/ntp_background-2.jpg"
        },
        "colors": {
            "frame": [ 11,11,11 ],
            "toolbar": [ 38, 38, 38 ],
            "tab_text": [ 152,152,152 ],
